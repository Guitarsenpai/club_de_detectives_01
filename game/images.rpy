    ## CTC
    image ctc_blink_nvl:
        zoom .8
        xoffset 10
        yoffset 13
        "gui/ctc.png"
        linear 1 alpha 0
        linear 1 alpha 1
        repeat
    image ctc_blink:
        "gui/ctc.png"
        xanchor 1.0
        yanchor 1.0
        xpos .980
        ypos .950
        block:
            linear .5 xoffset 20
            linear .5 xoffset 0
            repeat

