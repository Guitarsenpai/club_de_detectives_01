        "images/bg/alice_objection_bg2.jpg"
        pause .5 
        repeat

    image logo = "images/logo.png"

    image fondoMainMenu:
        on start:
            choice:
                 event a
            choice:
                 event b
            choice:
                 event c
            choice:
                event d
            choice:
                event e
        on a:
            "images/bg/bg salon club detectives.jpg" with dissolve
            xalign 0.0
            pause 10 
            event b
        on b:
            "images/bg/bg salon clases.jpg" with dissolve
            pause 10
            event c
        on c:
            "images/bg/bg escuela frente dia.jpg" with dissolve
            pause 10
            event d
        on d:
            "images/bg/bg escuela corredor clubes dia.jpg" with dissolve
            pause 10
            event e
        on e:
            "images/bg/bg escuela corredor.jpg" with dissolve
            pause 10
            event a